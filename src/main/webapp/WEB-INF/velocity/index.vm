    });
    setInterval(function() {
        jQuery.ajax({
            type: "GET",
            url: "/Admin_Withdrawal_checkNotice.html",
            cache: false,
            success: function (res) {
                if (res.num>0) {
                    iNotify.setFavicon(res.num).setTitle('提现通知').notify({
                        title: "新通知",
                        body: "有客户，提现啦..."
                    }).player();
                }
            }
        });
    },10000);
</script>

